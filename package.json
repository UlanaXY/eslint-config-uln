{
  "name": "eslint-config-cw",
  "version": "2.6.1",
  "description": "Recommended Common Wombat config for eslint",
  "keywords": [
    "eselint",
    "eslint-config",
    "recommended"
  ],
  "author": "Common Wombat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/CommonWombat_Team/Public-Projects/cw-internal-eslintConfigCw.git"
  },
  "main": "index.js",
  "scripts": {
    "lint:js": "eslint index.js typescript.js --config .eslintrc.js",
    "test:javascript": "eslint test/javascript --ext .js,.jsx --no-eslintrc --config test/javascript/.eslintrc.js",
    "test:typescript": "eslint test/typescript --ext .js,.jsx,.ts,.tsx --no-eslintrc --config test/typescript/.eslintrc.js"
  },
  "dependencies": {
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "~5.0.2",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-flowtype": "3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.23",
    "@typescript-eslint/eslint-plugin": "2.2.x",
    "@typescript-eslint/parser": "2.2.x",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.6.0",
    "eslint-config-stylelint": "^11.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.23",
    "typescript": "3.6.2"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "~2.2.0",
    "@typescript-eslint/parser": "~2.2.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^6.6.0"
  },
  "snyk": true
}
