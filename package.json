{
  "name": "eslint-config-uln",
  "version": "6.0.0",
  "description": "UlanaXY's recommended config for eslint",
  "keywords": [
    "eselint",
    "eslint-config",
    "recommended"
  ],
  "author": "ulanaXY",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/UlanaXY/eslint-config-uln"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "type": "module",
  "main": "src/typescript.js",
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "lint:js": "eslint ./src --config eslint.config.mjs",
    "test": "yarn run test:javascript && yarn run test:typescript",
    "test:typescript": "eslint ./test/typescript --config test/typescript/eslint.config.mjs"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.4.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.12.0",
    "typescript-eslint": "^7.12.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "eslint": "^8.57.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "typescript": "~5.5.2"
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "typescript": "~5.5.2"
  },
  "snyk": true
}
