{
  "name": "eslint-config-uln",
  "version": "5.1.1",
  "description": "Recommended UlanaXY config for eslint",
  "keywords": [
    "eselint",
    "eslint-config",
    "recommended"
  ],
  "author": "ulanaXY",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/UlanaXY/eslint-config-uln"
  },
  "main": "typescript.js",
  "scripts": {
    "lint:js": "eslint base.js javascript.js typescript.js --config .eslintrc.js",
    "test": "yarn run test:javascript && yarn run test:typescript",
    "test:javascript": "eslint test/javascript ./test/javascript --ext .js,.jsx --no-eslintrc --config test/javascript/.eslintrc.js",
    "test:typescript": "eslint test/typescript ./test/typescript --ext .js,.jsx,.ts,.tsx --no-eslintrc --config test/typescript/.eslintrc.js",
    "snyk-protect": "snyk protect",
    "prepare": "test -n \"$IS_CI\" || yarn run snyk-protect"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-airbnb-typescript": "^15.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/eslint-parser": "^7.10.0",
    "@babel/preset-react": "^7.13.13",
    "@types/react": "^16.9.35",
    "eslint": "^8.2.0",
    "eslint-config-stylelint": "^11.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.23",
    "typescript": "^4.0.0",
    "webpack": "^4.43.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "eslint": "^8.2.0"
  },
  "snyk": true
}
