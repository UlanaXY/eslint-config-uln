{
  "name": "eslint-config-wombat",
  "version": "5.0.0",
  "description": "Recommended Common Wombat config for eslint",
  "keywords": [
    "eselint",
    "eslint-config",
    "recommended"
  ],
  "author": "Common Wombat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/CommonWombat/public-projects/cw-eslintConfig-config.git"
  },
  "main": "typescript.js",
  "scripts": {
    "lint:js": "eslint base.js javascript.js typescript.js --config .eslintrc.js",
    "test": "yarn run test:javascript && yarn run test:typescript",
    "test:javascript": "eslint test/javascript ./test/javascript --ext .js,.jsx --no-eslintrc --config test/javascript/.eslintrc.js",
    "test:typescript": "eslint test/typescript ./test/typescript --ext .js,.jsx,.ts,.tsx --no-eslintrc --config test/typescript/.eslintrc.js",
    "createNewVersion": "np --skip-2fa",
    "snyk-protect": "snyk protect",
    "prepare": "test -n \"$IS_CI\" || yarn run snyk-protect"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/eslint-parser": "^7.10.0",
    "@babel/preset-react": "^7.13.13",
    "@types/react": "^16.9.35",
    "eslint": "^7.5.0",
    "eslint-config-stylelint": "^11.0.0",
    "np": "^7.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.23",
    "typescript": "~4.0.0",
    "webpack": "^4.43.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "eslint": "^7.32.0"
  },
  "snyk": true
}
